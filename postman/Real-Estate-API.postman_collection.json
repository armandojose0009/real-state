  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{access_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "access_token",
      "value": "",
      "type": "string"
    },
    {
      "key": "property_id",
      "value": "",
      "type": "string"
    },
    {
      "key": "listing_id",
      "value": "",
      "type": "string"
    },
    {
      "key": "transaction_id",
      "value": "",
      "type": "string"
    },
    {
      "key": "tenant_id",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@admin.com\",\n  \"password\": \"Password123!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/v1/auth/login",
              "host": ["{{base_url}}"],
              "path": ["v1", "auth", "login"]
            }
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"your_refresh_token_here\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/v1/auth/refresh",
              "host": ["{{base_url}}"],
              "path": ["v1", "auth", "refresh"]
            }
          }
        }
      ]
    },
    {
      "name": "Properties",
      "item": [
        {
          "name": "Create Property",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"address\": \"123 Main St\",\n  \"city\": \"Miami\",\n  \"state\": \"FL\",\n  \"zipCode\": \"33101\",\n  \"sector\": \"Downtown\",\n  \"propertyType\": \"Apartment\",\n  \"location\": {\n    \"type\": \"Point\",\n    \"coordinates\": [-80.1918, 25.7617]\n  },\n  \"valuation\": 350000,\n  \"bedrooms\": 2,\n  \"bathrooms\": 2,\n  \"squareFeet\": 1200,\n  \"yearBuilt\": 2020\n}"
            },
            "url": {
              "raw": "{{base_url}}/v1/properties",
              "host": ["{{base_url}}"],
              "path": ["v1", "properties"]
            }
          }
        },
        {
          "name": "Get All Properties (Paginated)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/v1/properties?limit=10&offset=0&sort=valuation&order=DESC",
              "host": ["{{base_url}}"],
              "path": ["v1", "properties"],
              "query": [
                {
                  "key": "limit",
                  "value": "10"
                },
                {
                  "key": "offset",
                  "value": "0"
                },
                {
                  "key": "sector",
                  "value": "Downtown",
                  "disabled": true
                },
                {
                  "key": "propertyType",
                  "value": "Apartment",
                  "disabled": true
                },
                {
                  "key": "minPrice",
                  "value": "200000",
                  "disabled": true
                },
                {
                  "key": "maxPrice",
                  "value": "500000",
                  "disabled": true
                },
                {
                  "key": "search",
                  "value": "Main",
                  "disabled": true
                },
                {
                  "key": "sort",
                  "value": "valuation"
                },
                {
                  "key": "order",
                  "value": "DESC"
                }
              ]
            }
          }
        },
        {
          "name": "Get Nearby Properties",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/v1/properties/nearby?lat=25.7617&lng=-80.1918&radius=5000",
              "host": ["{{base_url}}"],
              "path": ["v1", "properties", "nearby"],
              "query": [
                {
                  "key": "lat",
                  "value": "25.7617"
                },
                {
                  "key": "lng",
                  "value": "-80.1918"
                },
                {
                  "key": "radius",
                  "value": "5000"
                }
              ]
            }
          }
        },
        {
          "name": "Get Property by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/v1/properties/{{property_id}}",
              "host": ["{{base_url}}"],
              "path": ["v1", "properties", "{{property_id}}"]
            }
          }
        },
        {
          "name": "Update Property",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"valuation\": 375000,\n  \"bedrooms\": 3\n}"
            },
            "url": {
              "raw": "{{base_url}}/v1/properties/{{property_id}}",
              "host": ["{{base_url}}"],
              "path": ["v1", "properties", "{{property_id}}"]
            }
          }
        },
        {
          "name": "Delete Property",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/v1/properties/{{property_id}}",
              "host": ["{{base_url}}"],
              "path": ["v1", "properties", "{{property_id}}"]
            }
          }
        }
      ]
    },
